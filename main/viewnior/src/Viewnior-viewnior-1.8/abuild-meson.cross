[binaries]
strip = 'i586-alpine-linux-musl-strip'
readelf = 'i586-alpine-linux-musl-readelf'
objcopy = 'i586-alpine-linux-musl-objcopy'
pkg-config = 'pkg-config'
[properties]
needs_exe_wrapper = true
sys_root = '/home/build/sysroot-x86/'
[host_machine]
system = 'linux'
cpu_family = 'x86'
cpu = 'x86'
endian = 'little'
